	Generator.space_users = (space_id, number, other_object)->
		console.time('init_space_users')
		main_org = Creator.getCollection('organizations').findOne({space: space_id, is_company: true}, {fields: {_id: 1}})

		if !main_org
			throw new Error('未找到is_company为true的部门')

		main_org_id = main_org._id

		user_docs = []

		space_user_docs = []

		customers = []

		other_docs = {}

		_.range(0, number).forEach ()->
			user_doc = getUserDoc()
			space_user_doc = getSpaceUserDoc(space_id, main_org_id, user_doc)

			customer = getCustomers(space_id, user_doc)

			user_docs.push(user_doc)
			space_user_docs.push(space_user_doc)
			customers.push(customer)

			other_object.forEach (object_name)->
				if !other_docs[object_name]
					other_docs[object_name] = []
				# 一个用户对应多条记录
				if object_name == 'love_work_experience' || object_name == 'love_educational_experience'
					_.range(0,_.random(0,5)).forEach ()->
						other_docs[object_name].push Generator.run(object_name, space_id, user_doc._id, user_doc.name)
				else
					#一个用户对应一条记录
					other_docs[object_name].push Generator.run(object_name, space_id, user_doc._id, user_doc.name)

		insertMany('users', user_docs)
		insertMany('space_users', space_user_docs)
		insertMany('vip_customers', customers)

		_.forEach other_docs, (other_doc, key)->
			insertMany(key, other_doc)

		console.timeEnd('init_space_users')


