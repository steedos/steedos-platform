	Generator.run = (object_name, space_id, owner, owner_name)->
		object = Creator.getObject(object_name);

		if !object
			return;

		if object_name == 'love_hobby'
			return getLoveHobbyDoc(space_id, owner)

		fields = object.fields;

		values = {_id: Creator.getCollection(object_name)._makeNewID(), _test_data: true}

		_.forEach fields, (f, k)->

			if k.startsWith('instances') || k.startsWith('sharing') || k.endsWith('_i')
				return ;

			if  k == 'owner'
				values[k] = owner
			else if k == 'space'
				values[k] = space_id
			else if object_name == 'love_about_me' && k == 'name'
				values[k] = owner_name
			else if f.type == 'text'
				values[k] = getText()
			else if f.type == 'textarea'
				values[k] = getTextarea()
			else if f.type == 'number'
				if f.scale > 0
					values[k] = getFloat()
				else
					values[k] = getNumber()
			else if f.type == 'select'
				if k.endsWith('_o')
					mk = k.substr(0, k.lastIndexOf('_o'))
					_v = []
					o_vs = _.pluck( fields[mk].options, 'value')
					_.range(0, o_vs.length).forEach (i)->
						if _.random(0,1) == 1
							_v.push(o_vs[i])

					values[k] = _v

					if _v.length == o_vs.length
						values[mk + '_i'] = 0
					else
						values[mk + '_i'] = _.random(1,2)
				else
					o_vs = _.pluck(f.options, 'value')
					index = _.random(0, o_vs.length - 1)
					values[k] = o_vs[index]
			else if f.type == 'date'
				date=new Date()
				date.setFullYear(_.random(1968, 2003), _.random(0,11), _.random(1,28))
				date.setHours(0)
				date.setMinutes(0)
				date.setSeconds(0)
				date.setMilliseconds(0)
				values[k] = date
			else if f.type == 'datetime'
				datetime=new Date()
				datetime.setFullYear(_.random(2017, 2020), _.random(0,11), _.random(1,28))
				datetime.setHours(_.random(1, 23))
				datetime.setMinutes(_.random(1, 59))
				datetime.setSeconds(_.random(1, 59))
				datetime.setMilliseconds(_.random(1, 999))
				values[k] = datetime
			else if f.type == 'selectCity'
				values[k] = getCity()


			if k.endsWith('_max') && f.type == 'select'
				mk = k.substr(0, k.lastIndexOf('_max'))
				mv = values[mk]
				o_vs = _.pluck(f.options, 'value')
				index = _.random(_.indexOf(o_vs, mv), o_vs.length - 1)
				values[k] = o_vs[index]


		if object_name == 'love_about_me'
			birthday = values['birthday']
			values['zodiac'] = getzodiac(birthday.getFullYear())
			values['constellation'] = getAstro(birthday.getMonth(), birthday.getDate())

		if object_name == 'love_educational_experience'
			values['name'] = getSchoolName()
			values['profession'] = getProfession()

		if object_name == 'love_work_experience'
			values['name'] = getCompanyName()
			values['position'] = getPosition()

		return values;
