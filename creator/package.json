{
  "name": "steedos",
  "version": "1.22.0-alpha.3",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules/steedos-server && rm -rf node_modules/@steedos",
    "start": "env-cmd -f ./.env.local meteor run --settings settings.json --port 3100",
    "start-verbose": "env-cmd -f ./.env.local meteor run --verbose --settings settings.json --port 3100",
    "build:css": "npx tailwindcss -o client/tailwind.css",
    "build": "yarn && rm -rf .dist/bundle && cross-env TOOL_NODE_FLAGS=--max-old-space-size=9800 meteor build --directory .dist --server-only && node .scripts/build-after.js",
    "build-debug": "yarn && rm -rf .dist/bundle && cross-env TOOL_NODE_FLAGS=--max-old-space-size=9800 meteor build --directory .dist --server-only --debug && node .scripts/build-after.js",
    "pub": "cd .dist && npm publish --registry https://registry.npmjs.org",
    "postinstall": "node .scripts/npm-postinstall.js"
  },
  "dependencies": {
