    {
      "name": "Finding if Date is in Current Quarter",
      "formula": "AND(\nCEILING( MONTH( date ) / 3 ) = CEILING( MONTH( today ) / 3 ),\nYEAR( date ) = YEAR( today )\n)",
      "suites": [
        {
          "context": "In current quarter",
          "identifiers": {
            "today": {
              "dataType": "date",
              "value": "2019-01-01"
