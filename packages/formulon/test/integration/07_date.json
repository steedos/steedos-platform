{
  "name": "Sample Date Formulas",
  "link": "https://help.salesforce.com/articleView?id=formula_examples_dates.htm",
  "examples": [
    {
      "name": "Finding Out if a Year Is a Leap Year",
      "formula": "OR(\nMOD( YEAR( date ), 400 ) = 0,\nAND(\nMOD( YEAR( date ), 4 ) = 0,\nMOD( YEAR( date ), 100 ) != 0\n)\n)",
      "suites": [
        {
          "context": "1984",
          "identifiers": {
            "date": {
              "dataType": "date",
              "value": "1984-01-25"
            }
          },
          "expectedResult": {
            "dataType": "checkbox",
            "value": true
          }
        },
        {
          "context": "1900",
          "identifiers": {
            "date": {
              "dataType": "date",
              "value": "1900-12-13"
            }
          },
          "expectedResult": {
            "dataType": "checkbox",
            "value": false
          }
        },
        {
          "context": "2000",
          "identifiers": {
            "date": {
              "dataType": "date",
              "value": "2000-08-23"
            }
          },
          "expectedResult": {
            "dataType": "checkbox",
            "value": true
          }
        }
      ]
    },
