{
  "name": "steedos-cli",
  "version": "2.7.15",
  "description": "Develop and run your enterprise apps in miniutes",
  "main": "dist/index.js",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version-disabled": "oclif-dev readme && git add README.md",
    "build": "rm -rf dist && tsc",
    "pub_next": "npm publish --tag next --registry https://registry.npmjs.org && start https://npm.taobao.org/sync/@steedos/steedos-cli"
  },
  "bin": {
    "steedos": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steedos/cli.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/steedos/cli/issues"
  },
  "homepage": "https://github.com/steedos/cli#readme",
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/default.steedos.config.js",
    "/default.steedos.settings.yml",
    "/dist"
  ],
  "dependencies": {
    "@geek/spinner": "1.0.0",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.8.3",
    "@oclif/plugin-help": "^5.1.12",
    "@steedos/metadata-core": "2.7.15",
    "@steedos/metadata-registrar": "2.7.15",
    "archiver": "^5.0.2",
    "chalk": "2.4.2",
    "change-case": "^3.1.0",
    "chokidar": "3.5.3",
    "colors": "^1.3.3",
    "compressing": "^1.5.1",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.6",
    "inquirer": "^6.2.2",
    "js-yaml": "^3.14.1",
    "kleur": "^4.1.3",
    "latest-version": "^5.1.0",
    "lodash": "^4.17.21",
    "moleculer": "^0.14.25",
    "readline-sync": "^1.4.10",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "rimraf": "3.0.2",
    "underscore": "^1.11.0",
    "validator": "^13.5.2",
    "yargs": "^13.2.1",
    "yn": "^2.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^2.1.0",
    "moleculer-repl": "^0.7.1"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "steedos",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "06a6c886ad4435088f40d07a836f12c93caed662"
}
